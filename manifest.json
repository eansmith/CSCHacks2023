{
  "name": "High Point Rate My Professor",
  "description": "Rate My Professor integration for Pitt High Point",
  "version": "1.0",
  "manifest_version": 3,
  "host_permissions": ["https://www.ratemyprofessors.com/graphql"],
  "content_scripts": [{
    "js": ["js/content.js"],
    "matches": ["https://pitcsprd.csps.pitt.edu/*"],
    "all_frames": true
  }],
  "background": {
    "service_worker": "js/apiCaller.js",
    "type": "module"
  },
  "action": {
    "default_icon": {
        "16": "lead_icon_16x16"
    },
    "default_popup": "/js/leader_popup.html"
  }
}